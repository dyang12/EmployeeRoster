<%= button_to "All Employees", employees_url, :method => :get %>
<h3>Edit Employee</h3>
<%= form_for @employee, :url => {:action => "update"} do |f| %>
  <input type="hidden" 
			 name="authenticity_token"
			 value="<%= form_authenticity_token %>">
			 
	<%= label_tag "employee[name]", "Name" %>
	<%= f.text_field :name %>
	<br>
	
	<%= label_tag "employee[email]", "Email" %>
	<%= f.text_field :email %>
	<br>
	
	<%= label_tag "employee[phone_number]", "Phone Number" %>
	<%= f.text_field :phone_number %>
	<br>
	
	<%= f.submit "Create" %>
<% end %>